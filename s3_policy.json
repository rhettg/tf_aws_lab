{
  "Version": "2012-10-17",
  "Id": "Policy1415115909152",
  "Statement": [
    {
      "Sid": "Access-from-specific-VPC",
      "Principal": "*",
      "Action": "s3:*",
      "Effect": "Allow",
      "Resource": ["arn:aws:s3:::${bucket_name}",
                   "arn:aws:s3:::${bucket_name}/*"],
      "Condition": {
        "StringEquals": {
          "aws:sourceVpc": "${vpc_id}"
        }
      }
    }
  ]
}	
